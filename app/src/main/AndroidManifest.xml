<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rpg">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RPG">
        <activity
            android:name=".graphic.MainActivity"
            android:screenOrientation="landscape"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".graphic.GameActivity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.StoreActivity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.Cave1Activity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.Cave1_1Activity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.InventoryActivity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.PeopleHomeActivity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.TransitionActivity"
            android:screenOrientation="landscape"
            />
        <activity
            android:name=".graphic.BattleManagerActivity"
            android:screenOrientation="landscape"
            />
    </application>

</manifest>